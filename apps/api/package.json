{
  "name": "@saas-rbac/api",
  "version": "1.0.0",
  "type": "module",
  "main": "src/server.ts",
  "types": "src/server.ts",
  "scripts": {
    "dev": "npm run env:load tsx watch src/server.ts",
    "start": "npm run env:load node dist/server.js",
    "build": "tsup src --out-dir dist",
    "db:seed": "npm run env:load npx prisma db seed",
    "db:studio": "npm run env:load npx prisma studio",
    "db:migratedev": "npm run env:load npx prisma migrate dev",
    "test": "npm run env:load vitest run --dir src",
    "test:watch": "npm run env:load vitest --dir src",
    "prettier:fix": "npx prettier . --write",
    "prettier:check": "npx prettier . --check",
    "env:load": "dotenv -e ../../.env --",
    "lint": "eslint --max-warnings=0",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [],
  "author": "devjuneo.com",
  "license": "ISC",
  "description": "SAAS RBAC Api",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@fastify/cookie": "11.0.2",
    "@fastify/cors": "10.0.2",
    "@fastify/jwt": "9.0.2",
    "@fastify/swagger": "9.4.1",
    "@fastify/swagger-ui": "5.2.1",
    "@prisma/client": "6.2.1",
    "@saas-rbac/auth": "*",
    "@saas-rbac/env": "*",
    "bcryptjs": "2.4.3",
    "chalk": "5.4.1",
    "fastify": "5.2.1",
    "fastify-i18n": "3.0.0",
    "fastify-plugin": "5.0.1",
    "fastify-type-provider-zod": "4.0.2",
    "lodash": "4.17.21",
    "resend": "4.0.1",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@faker-js/faker": "9.3.0",
    "@saas-rbac/eslint-config": "*",
    "@saas-rbac/prettier-config": "*",
    "@saas-rbac/typescript-config": "*",
    "@types/bcryptjs": "2.4.6",
    "@types/lodash": "4.17.14",
    "@types/node": "22.10.5",
    "dotenv-cli": "8.0.0",
    "prisma": "6.2.1",
    "tsup": "8.3.5",
    "tsx": "4.19.2",
    "typescript": "5.7.3",
    "vite": "5.4.11",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "2.1.8"
  }
}